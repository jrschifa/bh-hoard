{
  "$schema": "http://json-schema.org/schema",
  "cli": "nx",
  "$id": "UtilLib",
  "title": "",
  "type": "object",
  "properties": {
    "name": {
      "type": "string",
      "description": "",
      "$default": {
        "$source": "argv",
        "index": 0
      },
      "x-prompt": "What name would you like to use?"
    },
    "directory": {
      "type": "string",
      "description": "A directory where the project is placed",
      "alias": "d",
      "x-prompt": {
        "message": "Which directory do you want the lib to be in?",
        "enum": [
          "store",
          "api",
          "shared"
        ]
      }
    }
  },
  "required": ["name"]
}
